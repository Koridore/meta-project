{
  "compilerOptions": {
    "target": "es2015",
    "lib": ["ESNext", "ES2015"],
    "module": "commonjs",
    "strict": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "jsx": "preserve",
    "skipLibCheck": true
  },
  "include": ["./"],
  "exclude": ["dist", "build", "node_modules"]
}
