{
  "name": "@lentix/tsconfigs",
  "version": "0.0.0",
  "description": "TSConfigs Base Files",
  "private": true,
  "main": "dist/index.json",
  "files": [
    "*.md",
    "*.json",
    "LICENSE",
    "src/*"
  ],
  "scripts": {
    "preinstall": "",
    "prepare": "",
    "build": "pnpm run build",
    "test": "pnpm run jest"
  },
  "devDependencies": {
    "tsconfig": "^7.0.0",
    "bases": "^0.2.1"
  },
  "dependencies": {},
  "prettier": {
    "endOfLine": "lf",
    "printWidth": 80,
    "semi": false,
    "tabWidth": 2,
    "trailingComma": "es5"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn lint",
      "pre-push": "yarn test"
    }
  }
}
